              "id": "windows/metrics-windows.perfmon-32d724ca-cdb7-4f2b-95d1-3ef0566fa6d1",
              "compiled_stream": {
                "metricsets": [
                  "perfmon"
                ],
                "condition": "${host.platform} == 'windows'",
                "perfmon.group_measurements_by_instance": false,
                "perfmon.ignore_non_existent_counters": false,
                "perfmon.refresh_wildcard_counters": false,
                "perfmon.queries": [
                  {
                    "object": "Process",
                    "instance": [
                      "*"
                    ],
                    "counters": [
                      {
                        "name": "% Processor Time",
                        "field": "cpu_perc",
                        "format": "float"
                      },
                      {
                        "name": "Working Set"
                      }
                    ]
                  }
                ],
                "period": "10s"
              }
